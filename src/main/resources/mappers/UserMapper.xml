<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.project.moving.dao.UserDao">

	<insert id="signUp" parameterType="User">
		insert into users
			(id, pw, nickname)
		values
			(#{id}, #{pw}, #{nickname})
	</insert>
	


</mapper>